<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyCustomWidget">
        <attr name="title" format="string" />
        <attr name="title_color" format="color" />
        <attr name="back_enable" format="boolean" />
        <attr name="custom_enable" format="boolean" />
        <attr name="custom_icon" format="reference" />
        <attr name="custom_onclick" format="string" />
        <attr name="item_type">
            <enum name="alone" value="0" />
            <enum name="above" value="1" />
            <enum name="mid" value="2" />
            <enum name="below" value="3" />
        </attr>
        <attr name="text" format="string" />
        <attr name="icon" format="reference" />
        <attr name="num" format="integer" />
        <attr name="background" format="reference|color" />
        <attr name="value" format="string" />
        <attr name="unit" format="string" />
        <attr name="mask" format="boolean" />
        <attr name="type" format="string" />
        
        <attr name="goTextColor" format="color" />
        <attr name="goIcon" format="reference" />
    </declare-styleable>

</resources>